************************************************************/
body {
  background: #f2f2f2;
}
rBox {
  position: relative;
}
.aBox {
  position: absolute;
}
h1 {
  margin: 0;
}
.borders {
  -webkit-border-radius: 5px;
  /* Saf3-4, iOS 1-3.2, Android ≤1.6 */
  -moz-border-radius: 5px;
  border-radius: 5px;
  /* Opera 10.5 IE9, Saf5, Chrome, FF4, iOS 4, Android 2.1+ */
  /* useful if you don't want a bg color from leaking outside the border: */
  -webkit-background-clip: padding-box;
  -moz-background-clip: padding;
  background-clip: padding-box;
}
a {
  color: #3a7ebb;
  white-space: nowrap;
}
.hover a {
  color: #ffffff;
}
.red {
  color: red;
}
.blue {
  color: #4391e5;
}
.gray {
  color: #9f9f9f;
}
.higher-gray {
  color: #747474;
}
.btn {
  -webkit-border-radius: 0;
  /* Saf3-4, iOS 1-3.2, Android ≤1.6 */
  -moz-border-radius: 0;
  border-radius: 0;
  /* Opera 10.5 IE9, Saf5, Chrome, FF4, iOS 4, Android 2.1+ */
  /* useful if you don't want a bg color from leaking outside the border: */
  -webkit-background-clip: padding-box;
  -moz-background-clip: padding;
  background-clip: padding-box;
}
.equalHeight {
  padding-bottom: 9999px;
  margin-bottom: -9999px;
}
label {
  font-weight: normal;
}
.clearfix:after {
  clear: both;
  content: ".";
  display: block;
  height: 0;
  visibility: hidden;
}
.form-group {
  overflow: hidden;
  margin-bottom: 10px;
}
.form-horizontal .control-label {
  padding-top: 4px;
  color: #9f9f9f;
}
.separate {
  color: #9f9f9f;
  padding: 0 6px;
}
.row-fluid .separate {
  float: left;
}
.input-group-sm > .form-control,
.input-group-sm > .input-group-addon {
  border-radius: 0;
  height: 27px;
}
.table {
  margin-bottom: 0;
}
.table.basic tr:nth-child(even),
.table.basic thead tr {
  background: #f5f5f5;
}
.table.basic tr th {
  color: #9f9f9f;
}
.table.basic tr td,
.table.basic tr th {
  line-height: 20px;
  padding: 5px;
  text-align: center;
  vertical-align: middle;
  font-weight: normal;
}
.table.basic thead tr th {
  border: 0;
}
.table.basic td {
  border: 0;
}
.form-control {
  border-radius: 0;
  height: 28px;
}
