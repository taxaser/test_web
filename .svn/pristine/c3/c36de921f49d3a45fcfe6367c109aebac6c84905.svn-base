.foot{
  margin-top:20px;
  height:125px;
  border-top:1px @border-color solid;
  border-bottom: 2px @light-border-color solid;
  @import "./modules/logo";
  .logo{
    margin-top:26px;
    a{
      background: url("@{bg_path}/logo_bottom.jpg") no-repeat;
    }
  }

  .right-text{
    margin-top:36px;
    text-align: right;
    line-height:28px;
    p{
      color:#747d83;
    }
    a{
      color:#747d83;
      padding:0 5px;
    }
  }
}