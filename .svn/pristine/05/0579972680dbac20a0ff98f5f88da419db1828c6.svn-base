body {
  background: url(../../img/wrap_bg.gif) repeat-y;
  border-right: 1px #d3dce0 solid;
  font-size: 12px;
}
.nav-box {
  float: left;
  border-right: 1px #dedede solid;
  width: 289px;
  height: 120%;
}
.nav-box li {
  list-style: none;
}
.nav-box .project-list {
  height: 120%;
  background: #313131;
  overflow: hidden;
  width: 90px;
  margin-top: -1px;
  float: left;
}
.nav-box .project-list > ul {
  margin: 0;
  padding: 0;
  border-bottom: 1px #545454 solid;
}
.nav-box .project-list > ul > li.active a {
  border-color: #111;
  background: #111;
}
.nav-box .project-list > ul > li a {
  outline: none;
  height: 78px;
  border-top: 1px #545454 solid;
  border-bottom: 1px #111 solid;
  display: block;
  text-align: center;
  color: #e4e4e4;
  text-shadow: 1px 1px 3px #000;
  transition: all 0.3s ease 0s;
  text-decoration: none;
}
.nav-box .project-list > ul > li a .glyphicon {
  color: #ccc;
  width: 26px;
  height: 26px;
  margin: 15px auto 0;
  display: block;
  font-size: 2em;
}
.nav-box .project-list > ul > li a:hover {
  border-color: #111;
  background: #111;
  text-decoration: none;
}
.nav-box nav {
  background: #e8edf0;
  margin-left: 90px;
  position: relative;
}
.nav-box nav.no-project {
  margin-left: 0;
}
.nav-box nav ul {
  margin: 0;
  padding: 0;
  border-bottom: 1px #f3f6f7 solid;
  margin-top: -1px;
}
.nav-box nav ul > li {
  font: bold 12px/38px Simsun, Helvetica, Arial, sans-serif;
  border-bottom: 1px #D3DCE0 solid;
  border-top: 1px #f3f6f7 solid;
}
.nav-box nav ul > li.current > a {
  color: #000080;
  font-weight: bold;
}
.nav-box nav ul > li a {
  padding-left: 30px;
  color: #313131;
  text-decoration: none;
  display: block;
  height: 38px;
  cursor: pointer;
}
.nav-box nav ul > li a span {
  margin-right: 5px;
}
.nav-box nav ul > li a i {
  width: 16px;
  height: 16px;
  margin-right: 10px;
}
.nav-box nav ul > li a i.works {
  background-position: -29px 0;
}
.nav-box nav ul > li ol {
  width: 100%;
  margin: 0;
  display: none;
  padding: 0;
}
.nav-box nav ul > li ol > li a {
  color: #5d8299;
  padding-left: 56px;
  position: relative;
}
.nav-box nav ul > li ol > li a span.line {
  top: 0;
  margin: 0;
  position: absolute;
  width: 2px;
  height: 38px;
  background: none;
  right: -1px;
  display: block;
  overflow: hidden;
}
.nav-box nav ul > li ol > li.hover {
  border-bottom: 1px #D3DCE0 solid;
  border-top: 1px #D3DCE0 solid;
}
.nav-box nav ul > li ol > li.hover a {
  padding-left: 51px;
  background: #f2f2f2;
  border-left: 5px #408ad9 solid;
}
.nav-box nav ul > li ol > li.hover a span.line {
  background: #f2f2f2;
}
/*nav-box over*/
