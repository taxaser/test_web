@charset "utf-8";
/* CSS Document */
body,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
dl,
dt,
dd,
ul,
ol,
li,
pre,
form,
fieldset,
legend,
button,
input,
textarea,
th,
td {
  margin: 0;
  padding: 0;
  word-wrap: break-word;
  font-style: "Helvetica Neue", Helvetica, Arial, sans-serif;
  color: #333;
}
.clear {
  clear: both;
}
img,
input,
button {
  border: 0;
  background: none;
}
img {
  font-size: 0;
  vertical-align: middle;
}
body,
button,
input,
select,
textarea {
  font: 12px/1.5 Lucida Grande, Tahoma, Verdana, Lucida, Arial, sans-serif, Helvetica, "宋体";
  color: #666;
}
h1,
h2,
h3,
h4.h5,
h6,
button,
input,
select,
textarea {
  font-size: 100%;
}
var,
em,
i {
  font-family: normal;
  font-style: normal;
}
pre {
  font-family: "Courier New", Courier, monospace;
}
small {
  font-size: 12px;
}
.bold {
  font-weight: bold;
}
.font14 {
  font-size: 14px;
}
.font16 {
  font-size: 16px;
}
hr {
  border: 1px solid #dedede;
}
.font12 {
  font-size: 12px;
}
ul,
ol {
  list-style: none;
}
a {
  text-decoration: none;
  color: #2a8ec9;
}
a:hover {
  text-decoration: none;
  color: #ff5500;
}
.blue {
  color: #005d93;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
.lay-out {
  width: 940px;
  margin: 0 auto;
}
.mt_10 {
  margin-top: 10px;
}
.mb_10 {
  margin-bottom: 10px;
}
.ml_10 {
  margin-left: 10px;
}
.mr_10 {
  margin-right: 10px;
}
.right,
.nav .leav-msg {
  float: right;
}
.left {
  float: left;
}
.logo-box {
  height: 96px;
  border-bottom: 1px #dcdcdc solid;
  overflow: hidden;
}
.logo-box .logo {
  margin-top: 12px;
}
.logo-box .logo a {
  width: 342px;
  height: 71px;
  display: block;
  background: url("/static/www/img/logo.jpg") no-repeat;
  text-indent: -100000px;
}
.logo-box .language {
  margin-top: 32px;
}
.logo-box .language a {
  height: 32px;
  padding-left: 36px;
  background: url("/static/www/img/icon.png") no-repeat;
  display: inline-block;
  line-height: 36px;
  margin-left: 10px;
}
.logo-box .language a.hover {
  color: #009944;
  font-weight: bold;
}
.logo-box .language a.chinese {
  background-position: 0 -46px;
}
.logo-box .language a.english {
  background-position: 0 -4px;
}
.nav {
  height: 46px;
  border-bottom: 1px #dcdcdc solid;
  -webkit-box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
  -moz-box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
  box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
}
.nav .leav-msg {
  padding: 0 20px;
  height: 24px;
  line-height: 24px;
  background: #009944;
  color: #fff;
  margin-top: 11px;
}
.nav .leav-msg:hover {
  background: #f8970b;
}
.nav ul.nav-list {
  width: 940px;
  height: auto;
  padding: 13px 0;
}
.nav ul.nav-list li {
  float: left;
  padding: 0 20px;
  font: 14px/16px "Helvetica Neue", Helvetica, Arial, sans-serif;
  border-left: 1px #dcdcdc solid;
  margin-left: -1px;
}
.nav ul.nav-list li a {
  color: #3a3a3a;
}
.nav ul.nav-list li a:hover {
  color: #009944;
}
.nav ul.nav-list li.pos {
  position: relative;
}
.nav ul.nav-list ol {
  position: absolute;
  -webkit-box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
  -moz-box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
  box-shadow: 0 2px 3px rgba(20, 20, 20, 0.5);
  top: 33px;
  left: 0;
  width: 230px;
  padding: 0 20px 20px 10px;
  background: #fff;
  border: 1px #dcdcdc solid;
  border-top: 0;
}
.nav ul.nav-list ol li {
  float: none;
  line-height: 30px;
  border-left: 0;
  padding-left: 15px;
  background: url('/static/www/img/icon.png') 3px -81px no-repeat;
}
.out-form {
  position: fixed;
  padding: 25px;
  border: 3px #4ca6dd solid;
  z-index: 999;
  width: 630px;
  background: #fff;
  top: 50%;
  left: 50%;
  margin-top: -170px;
  margin-left: -315px;
}
.out-form .closed {
  position: absolute;
  right: -48px;
  top: -3px;
  width: 45px;
  height: 45px;
  background: #4ca6dd;
  cursor: pointer;
}
.out-form .closed a {
  background: url("/static/www/img/icon.png") no-repeat;
  background-position: 0 -155px;
  width: 24px;
  height: 24px;
  display: block;
  margin: 10px 0 0 10px;
}
.out-form h3 {
  font: 18px/30px 'å¾®è½¯é›…é»‘';
  color: #52a3d9;
}
.out-form h4 {
  margin: 10px 0;
}
.out-form .form-box {
  width: 626px;
  border: 2px #e9e9e9 solid;
  height: 213px;
}
.out-form .form-box .text {
  height: 30px;
  width: 96%;
  line-height: 30px;
  padding: 0 2%;
  border-bottom: 1px #dedede solid;
}
.out-form .form-box textarea {
  width: 96%;
  border: 0;
  resize: none;
  padding: 0 2%;
  height: 50px;
  line-height: 30px;
}
.out-form .form-box dt {
  height: 29px;
  border-bottom: 1px #e9e9f2 solid;
  line-height: 29px;
  padding-left: 15px;
  background: #f4f4f7;
  color: #333;
}
.out-form .form-box .important textarea {
  height: 50px;
}
.out-form .form-box .important dt {
  color: #ff0000;
}
.out-form .out-submit {
  margin-top: 10px;
  text-align: center;
  height: 49px;
}
.out-form .out-submit .bg-line {
  height: 2px;
  overflow: hidden;
}
.out-form .out-submit .submit {
  width: 175px;
  height: 47px;
  margin: 0 auto;
  overflow: hidden;
  background: #f89f13;
  color: #fff;
  cursor: pointer;
}
.content {
  padding: 15px 0;
  position: relative;
}
.index-banner {
  height: 408px;
  overflow: hidden;
  position: relative;
}
.index-banner .box {
  height: 408px;
  border-bottom: 4px #fff solid;
  overflow: hidden;
  position: relative;
}
.index-banner .box li a {
  height: 408px;
  display: block;
  overflow: hidden;
}
.index-banner .shadow {
  width: 1000px;
  height: 26px;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 20;
}
.index-banner .contrl {
  height: 26px;
  position: absolute;
  top: 330px;
}
.index-banner .contrl ul {
  float: right;
}
.index-banner .contrl ul li {
  display: inline;
  position: relative;
  float: left;
  margin-left: 5px;
  width: 26px;
  height: 26px;
}
.index-banner .contrl ul li a {
  width: 26px;
  left: 0;
  top: 0;
  position: absolute;
  height: 26px;
  display: block;
  background: #fff;
  border-radius: 10px;
  opacity: .5;
}
.index-banner .contrl ul li.hover a {
  background: orange;
}
.block .title {
  height: 40px;
  position: relative;
  margin: 15px 0;
  background: url('/static/www/img/line_bg.gif') left bottom repeat-x;
}
.block .title h2 {
  font: 24px/24px "微软雅黑";
  float: left;
  padding: 0 25px 12px 4px;
  border-bottom: 4px #f89f13 solid;
}
.block .title .right-control {
  float: right;
  margin-top: 3px;
}
.block .title .right-control a {
  display: inline-block;
  text-indent: -99999px;
  width: 19px;
  height: 18px;
  background: url('/static/www/img/icon.png') no-repeat;
}
.block .title .right-control a.prev {
  background-position: 0 -133px;
}
.block .title .right-control a.prev:hover {
  background-position: 0 -114px;
}
.block .title .right-control a.next {
  background-position: -20px -133px;
}
.block .title .right-control a.next:hover {
  background-position: -20px -114px;
}
.block .title .orange-line {
  position: absolute;
  bottom: 0;
}
.products {
  overflow: hidden;
}
.product-img {
  position: relative;
  height: 250px;
  border-bottom: 1px #dcdcdc solid;
  margin-bottom: 15px;
}
.product-img ul.product-list {
  position: absolute;
  margin-left: -12px;
}
.product-img ul.product-list li {
  float: left;
  display: inline;
  margin-right: 12px;
  width: 226px;
  height: 250px;
  text-align: center;
}
.product-img ul.product-list li .name {
  font-size: 15px;
  margin: 10px 0;
  height: 20px;
  overflow: hidden;
}
.product-img ul.product-list li .name a {
  color: #3a3a3a;
}
.product-img ul.product-list li .name a:hover {
  color: #009944;
}
.product-img ul.product-list li .text {
  text-align: center;
  height: 40px;
  line-height: 20px;
}
.product-img ul.product-list li img {
  width: 226px;
  height: 142px;
  display: block;
}
.about {
  padding: 20px 0 0 0;
  height: 264px;
  overflow: hidden;
}
.about .left-img {
  width: 422px;
  height: 264px;
  background: url('/static/www/img/about.jpg') center top no-repeat;
}
.about .text-box {
  width: 454px;
  line-height: 30px;
}
.about .text-box .tit {
  height: 27px;
  background: url('/static/www/img/about_title.jpg') center top no-repeat;
  margin-bottom: 20px;
}
.foot {
  margin-top: 20px;
  height: 125px;
  border-top: 1px #dcdcdc solid;
  border-bottom: 2px #f89f13 solid;
  background: url("/static/www/img/bottom_bg.jpg") repeat-x;
  margin-bottom: 60px;
}
.foot .logo {
  margin-top: 12px;
}
.foot .logo a {
  width: 342px;
  height: 71px;
  display: block;
  background: url("/static/www/img/logo.jpg") no-repeat;
  text-indent: -100000px;
}
.foot .logo {
  margin-top: 26px;
}
.foot .logo a {
  background: url("/static/www/img/logo_bottom.jpg") no-repeat;
}
.foot .right-text {
  margin-top: 36px;
  text-align: right;
  line-height: 28px;
}
.foot .right-text p {
  color: #747d83;
}
.foot .right-text a {
  color: #747d83;
  padding: 0 5px;
}
